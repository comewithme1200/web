        @RenderBody()
        <!-- End Main -->
    </div>
    <!-- End Wrap -->
    <!-- Begin Footer -->
    @Html.Partial("NguyenVuLong_Footer")
    <!-- End Footer -->

    @RenderSection("scripts", required: false)

    <script type="text/javascript">
        $("#delete").on("click", function () {


        })
    </script>
</body>
</html>

